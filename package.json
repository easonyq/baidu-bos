{
  "name": "baidu-bos",
  "version": "1.0.6",
  "description": "简易的上传 BOS 的命令行工具，用法同 edp bos",
  "main": "./bin/baidu-bos.js",
  "scripts": {
    "dev": "rimraf dist && babel src --out-dir dist --copy-files --watch",
    "prepublish": "npm run build",
    "build": "rimraf dist && babel src --out-dir dist --copy-files",
    "test": "nyc ava",
    "cover:html": "nyc ava -v && nyc report --reporter=html",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "bin": {
    "baidu-bos": "./bin/baidu-bos.js"
  },
  "ava": {
    "serial": false,
    "failFast": true,
    "concurrency": 10,
    "files": [
      "test/**/*.test.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "exclude": [
      "test/**/*.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/easonyq/baidu-bos.git"
  },
  "keywords": [
    "baidu",
    "bos"
  ],
  "files": [
    "bin",
    "dist"
  ],
  "author": "wangyisheng@outlook.com",
  "license": "ISC",
  "engines": {
    "node": ">= 7.0.0",
    "npm": ">= 5.0.0"
  },
  "dependencies": {
    "@baiducloud/sdk": "^1.0.0-rc.9",
    "axios": "^0.18.0",
    "babel-runtime": "^6.26.0",
    "chalk": "^2.4.1",
    "commander": "^2.17.1",
    "fs-extra": "^8.0.0",
    "glob": "^7.1.2",
    "mime": "^2.3.1",
    "semver": "^5.5.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "coveralls": "^3.0.2",
    "nyc": "^12.0.2",
    "rimraf": "^2.6.2"
  }
}
